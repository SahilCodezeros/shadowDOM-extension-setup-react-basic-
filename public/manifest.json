{
  "short_name": "Trailit",
  "name": "Trailit",
  "icons": {
    "128": "./images/icon128.png"
  },
  "background": {
    "scripts": ["app/background.js"]
  },

  "externally_connectable": {
    "matches": ["*://localhost/*", "*://169.61.16.14/*"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>", "http://*/*", "https://*/*"],
      "css": ["/static/css/content.css"],
      "js": ["/static/js/content.js"],
      "run_at": "document_end"
    }
  ],
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "key": "",
  "permissions": ["storage", "activeTab", "notifications", "<all_urls>"],
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk=';script-src-elem *; object-src 'self';"
}
